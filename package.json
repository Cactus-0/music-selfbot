{
    "name": "cactus-music-selfbot",
    "description": "Music selfbot for discord",
    "main": "dist/main",
    "bin": "./dist/main.js",
    "version": "1.0.1",
    "scripts": {
        "prebuild": "rm .tsbuildinfo & rm -rf dist",
        "watch": "npm run prebuild && tsc-watch --onSuccess \"node -r source-map-support/register .\"",
        "build": "bash ./scripts/build.sh",
        "start:dev": "NODE_ENV=development node -r source-map-support/register .",
        "start:prod": "node ."
    },
    "dependencies": {
        "@discordjs/opus": "^0.1.0",
        "@discordjs/voice": "^0.10.0",
        "autobind-decorator": "^2.4.0",
        "avconv": "^3.1.0",
        "axios": "^0.27.2",
        "colors": "^1.4.0",
        "discord.ts-selfbot": "^1.0.4-dev",
        "ffmpeg": "^0.0.4",
        "ffmpeg-static": "^4.4.1",
        "isomorphic-unfetch": "^3.1.0",
        "require-context": "^1.1.0",
        "spotify-url-info": "^3.1.2",
        "tsconfig-paths": "^3.14.1",
        "youtube-dl-exec": "^2.0.12",
        "youtube-search-without-api-key": "^1.0.7",
        "ytdl-core": "^4.11.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.21",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.21",
        "tsc-watch": "^5.0.3",
        "tslib": "^2.3.1",
        "typed-emitter": "^2.1.0",
        "typescript": "4.6.2"
    },
    "pkg": {
        "scripts": "dist/**/*.js",
        "assets": "node_modules/**/*",
        "targets": [
            "node16-linux-x64"
        ],
        "outputPath": "bin"
    }
}
